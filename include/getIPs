function getIPs(sheetId) {
  // Initialize the Google Sheets API client
  const sheets = google.sheets({ version: 'v4', auth: authClient });

  try {
    // Get the range of cells containing the IP addresses
    const range = 'A2:B';

    // Make the request to get the values of the range
    const response = await sheets.spreadsheets.values.get({
      spreadsheetId: sheetId,
      range: range,
    });

    const rows = response.data.values;

    // Create an object that maps hostnames to IP addresses
    const ips = {};
    rows.forEach((row) => {
      ips[row[0]] = row[1];
    });

    // Return the object containing the IP addresses
    return ips;
  } catch (error) {
    console.error('Error getting IPs:', error);
    return null;
  }
}
